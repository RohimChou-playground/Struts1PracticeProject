<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">

<struts-config>
    <action-mappings>
        <!-- whenever request this url: http://xxx/xx/subscriptions.do, the request will be mapped to SubscriServlet.java -->
        <action path="/subscriptions" type="com.mycompany.actions.SubscriServlet"></action>
    </action-mappings>
</struts-config>